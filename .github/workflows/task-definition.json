{
  "family": "hello-world-task",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "demo-deploy",
      "image": "${432606891118}.dkr.ecr.${ap-south-1}.amazonaws.com/${demo-deploy}:${latest}",
      "essential": true,
      "memory": 256,
      "cpu": 256,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::${432606891118}:role/ecsTaskExecutionRole"
}
